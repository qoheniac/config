# SPECIAL SYMBOLS
# Note: press letter first then combination for vector arrow
<Multi_key> <backslash> <comma> : " " U202F # NARROW NO-BREAK SPACE
<Multi_key> <v> : "⃗" U20D7 # COMBINING RIGHT ARROW ABOVE
<Multi_key> <asciicircum> : "̂" U0302 # COMBINING CIRCUMFLEX ACCENT

###################################################################
#                          G R E E K                              #
###################################################################
# Note:
#   - pi for π, f for ϕ, p for ψ
#   - th for θ, t for τ
#   - e for ϵ, ä for η
#   - no omikron (like latin o)
# (same goes for variants and capital letters)

# SMALL GREEK LETTERS
<Multi_key> <g> <a> : "α" U03B1 # GREEK SMALL LETTER ALPHA
<Multi_key> <g> <b> : "β" U03B2 # GREEK SMALL LETTER BETA
<Multi_key> <g> <g> : "γ" U03B3 # GREEK SMALL LETTER GAMMA
<Multi_key> <g> <d> : "δ" U03B4 # GREEK SMALL LETTER DELTA
<Multi_key> <g> <e> : "ϵ" U03F5 # GREEK LUNATE EPSILON SYMBOL
<Multi_key> <g> <z> : "ζ" U03B6 # GREEK SMALL LETTER ZETA
<Multi_key> <g> <adiaeresis> : "η" U03B7 # GREEK SMALL LETTER ETA
<Multi_key> <g> <t> <h> : "θ" U03B8 # GREEK SMALL LETTER THETA
<Multi_key> <g> <i> : "ι" U03B9 # GREEK SMALL LETTER IOTA
<Multi_key> <g> <k> : "κ" U03BA # GREEK SMALL LETTER KAPPA
<Multi_key> <g> <l> : "λ" U03BB # GREEK SMALL LETTER LAMDA
<Multi_key> <g> <m> : "μ" U03BC # GREEK SMALL LETTER MU
<Multi_key> <g> <n> : "ν" U03BD # GREEK SMALL LETTER NU
<Multi_key> <g> <x> : "ξ" U03BE # GREEK SMALL LETTER XI
<Multi_key> <g> <p> <i> : "π" U03C0 # GREEK SMALL LETTER PI # Attention! (cf. ψ)
<Multi_key> <g> <r> : "ρ" U03C1 # GREEK SMALL LETTER RHO
<Multi_key> <g> <s> : "σ" U03C3 # GREEK SMALL LETTER SIGMA
<Multi_key> <g> <t> : "τ" U03C4 # GREEK SMALL LETTER TAU
<Multi_key> <g> <y> : "υ" U03C5 # GREEK SMALL LETTER UPSILON
<Multi_key> <g> <f> : "ϕ" U03D5 # GREEK PHI SYMBOL
<Multi_key> <g> <c> : "χ" U03C7 # GREEK SMALL LETTER CHI
<Multi_key> <g> <p> : "ψ" U03C8 # GREEK SMALL LETTER PSI
<Multi_key> <g> <o> : "ω" U03C9 # GREEK SMALL LETTER OMEGA

# VARIANTS OF SMALL GREEK LETTERS
<Multi_key> <g> <v> <b> : "ϐ" U03D0 # GREEK BETA SYMBOL
<Multi_key> <g> <v> <e> : "ε" U03B5 # GREEK SMALL LETTER EPSILON
<Multi_key> <g> <v> <t> <h> : "ϑ" U03D1 # GREEK THETA SYMBOL # GREEK SMALL LETTER THETA # Attention! (cf. θ)
<Multi_key> <g> <v> <k> : "ϰ" U03F0 # GREEK KAPPA SYMBOL
<Multi_key> <g> <v> <p> <i> : "ϖ" U03D6 # GREEK PI SYMBOL
<Multi_key> <g> <v> <r> : "ϱ" U03F1 # GREEK RHO SYMBOL
<Multi_key> <g> <v> <s> : "ς" U03C2 # GREEK SMALL LETTER FINAL SIGMA
<Multi_key> <g> <v> <f> : "φ" U03C6 # GREEK SMALL LETTER PHI # GREEK PHI SYMBOL # Attention! (cf. ϕ)

# CAPITAL GREEK LETTERS (as far as they are different from latin ones)
<Multi_key> <g> <G> : "Γ" U0393 # GREEK CAPITAL LETTER GAMMA
<Multi_key> <g> <D> : "Δ" U0394 # GREEK CAPITAL LETTER DELTA
<Multi_key> <g> <T> <H> : "Θ" U0398 # GREEK CAPITAL LETTER THETA # GREEK SMALL LETTER THETA # Attention! (cf. θ)
<Multi_key> <g> <L> : "Λ" U039B # GREEK CAPITAL LETTER LAMDA
<Multi_key> <g> <X> : "Ξ" U039E # GREEK CAPITAL LETTER XI
<Multi_key> <g> <P> <I> : "Π" U03A0 # GREEK CAPITAL LETTER PI # GREEK SMALL LETTER PI # Attention! (cf. π)
<Multi_key> <g> <S> : "Σ" U03A3 # GREEK CAPITAL LETTER SIGMA
<Multi_key> <g> <Y> : "ϒ" U03D2 # GREEK UPSILON WITH HOOK SYMBOL
<Multi_key> <g> <F> : "Φ" U03A6 # GREEK CAPITAL LETTER PHI # GREEK SMALL LETTER PHI # Attention! (cf. φ)
<Multi_key> <g> <P> : "Ψ" U03A8 # GREEK CAPITAL LETTER PSI
<Multi_key> <g> <O> : "Ω" U03A9 # GREEK CAPITAL LETTER OMEGA
